  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@microsoft/api-documenter": "^7.7.8",
    "@microsoft/api-extractor": "^7.7.5",
    "@rocketbase/rollup-plugin-api-extractor": "^1.0.3",
    "@rocketbase/rollup-plugin-exec": "^1.0.1",
    "@rocketbase/rollup-plugin-sequential": "^1.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.3",
    "@semantic-release/npm": "^7.0.3",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@wessberg/rollup-plugin-ts": "^1.2.13",
    "bestzip": "^2.1.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "jest-preset-typescript": "^1.2.0",
    "lint-staged": "^10.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
